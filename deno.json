{
  "name": "@claudiu-ceia/ts-duckling",
  "version": "0.3.5",
  "exports": {
    ".": "./mod.ts"
  },
  "publish": {
    "exclude": [
      ".github/",
      "bench/",
      "coverage/",
      "docs/",
      "scripts/",
      "tests/"
    ]
  },
  "imports": {
    "@claudiu-ceia/combine": "jsr:@claudiu-ceia/combine@0.2.8",
    "@std/assert": "jsr:@std/assert@1.0.18",
    "@std/testing/time": "jsr:@std/testing@1.0.17/time",

    "@data/countries-en-us": "./data/countries-en-us.json",
    "@data/languages-en": "./data/languages-en.json",
    "@data/tlds": "./data/tlds.json"
  },
  "tasks": {
    "fmt": "deno fmt",
    "lint": "deno lint",
    "check": "deno fmt && deno lint && deno test",
    "update:data": "deno run -A scripts/update_data.ts",
    "test": "deno test",
    "hooks:install": "ln -sf ../../scripts/pre-commit .git/hooks/pre-commit && chmod +x scripts/pre-commit",
    "hooks:uninstall": "rm -f .git/hooks/pre-commit"
  },
  "exclude": ["./docs/"]
}
